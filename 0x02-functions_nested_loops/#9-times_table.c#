#include "holberton.h"

/**
 * times_table - prints the 9 times table, starting with 0,
 *
 * Return: nothing to return.
 */
void times_table(void)
{
	int i, j, r;

	for (i = 0; i < 10; i++)
	{
		for (j = 0; j < 10; j++)
		{
				_putchar(48 + i);
				_putchar('x');
				_putchar(48 + j);
				_putchar('=');
				_putchar(48 + (r / 10));
				_putchar(48 + (r % 10));
				_putchar(',');
			}
	}
}
